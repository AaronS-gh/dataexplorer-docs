{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.svg",
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      },
      {
        "files": [
          "includes/**/*.svg",
          "includes/**/*.png",
          "includes/**/*.jpg",
          "includes/**/*.jpeg",
          "includes/**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/_themes/**"
        ],
        "src": null,
        "dest": null
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "searchScope": ["Kusto", "kusto", "KQL"],
      "breadcrumb_path": "/kusto/breadcrumb/toc.json",
      "feedback_system": "Standard",
      // "feedback_product_url": "https://feedback.azure.com/d365community/forum/a952add5-eb24-ec11-b6e6-000d3a4f0da0",
      // "feedback_help_link_url": "https://learn.microsoft.com/answers/topics/azure-data-explorer.html",
      // "feedback_help_link_type": "get-help-at-qna",
      "recommendations": true,
      "_op_documentIdPathDepotMapping": {
        "./kusto": {
          "depot_name": "MSDN.kustolang",
